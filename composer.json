{
    "name": "munindra-ai/laravel-useful-commands",
    "description": "This is my package laravel-useful-commands",
    "keywords": [
        "munindra-ai",
        "laravel-useful-commands"
    ],
    "homepage": "https://github.com/munindra-ai/laravel-useful-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "munindra-ai",
            "email": "manindratamang5@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Munindra-ai\\LaravelUsefulCommands\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Munindra-ai\\LaravelUsefulCommands\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
